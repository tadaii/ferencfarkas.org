{
  "name": "ferencfarkas.org",
  "version": "1.2.0",
  "description": "The ferencfarkas.org project",
  "private": true,
  "scripts": {
    "start": "npm run website:serve",
    "dev": "run-p webapp:serve website:serve",
    "prebuild": "node ./scripts/prebuild-web",
    "webapp:serve": "cd webapp && npm run dev",
    "webapp:build": "cd webapp && npm run build && cd ..",
    "website:serve": "npm run prebuild -- dev && cd ./website && hugo server",
    "catalogue:pdf": "node ./catalogue/pdf-generator/index.js"
  },
  "dependencies": {
    "@babel/cli": "^7.12.1",
    "@babel/core": "^7.14.3",
    "@babel/plugin-transform-runtime": "^7.14.3",
    "@babel/preset-env": "^7.14.4",
    "@rollup/plugin-commonjs": "^19.0.0",
    "@rollup/plugin-node-resolve": "^13.0.0",
    "autoprefixer": "^10.2.6",
    "date-fns": "^2.22.1",
    "eslint": "^7.27.0",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-promise": "^5.1.0",
    "eslint-plugin-standard": "^4.1.0",
    "front-matter": "^4.0.2",
    "fs-extra": "^10.0.0",
    "isomorphic-git": "^1.8.2",
    "lunr": "^2.3.9",
    "lunr-languages": "^1.4.0",
    "pdfmake": "^0.1.71",
    "postcss-cli": "^8.3.1",
    "postcss-custom-media": "^8.0.0",
    "prettier": "^2.3.0",
    "rollup": "^2.50.6",
    "rollup-plugin-babel": "^4.4.0",
    "rollup-plugin-css-only": "^3.1.0",
    "rollup-plugin-livereload": "^2.0.0",
    "rollup-plugin-svelte": "^7.1.0",
    "rollup-plugin-terser": "^7.0.2",
    "shelljs": "^0.8.4",
    "sirv-cli": "^1.0.12",
    "standard": "^16.0.3",
    "svelte": "^3.38.2",
    "wavesurfer.js": "^5.0.1",
    "yaml": "^1.10.2"
  },
  "devDependencies": {
    "npm-run-all": "^4.1.5"
  }
}
