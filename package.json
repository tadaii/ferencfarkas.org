{
  "name": "ferencfarkas.org",
  "version": "1.7.93",
  "description": "The ferencfarkas.org project",
  "private": false,
  "scripts": {
    "start": "npm run website:serve",
    "dev": "run-p webapp:serve website:serve",
    "work:setdate": "node ./scripts/workdate",
    "prebuild": "node ./scripts/prebuild",
    "release": "node ./scripts/release",
    "release:check": "node ./scripts/release check",
    "scores": "node ./scripts/scores",
    "scores:diff": "node ./scripts/scores diff",
    "webapp:serve": "cd webapp && npm run dev",
    "webapp:build": "cd webapp && npm run build && cd ..",
    "website:serve": "npm run prebuild -- dev && cd ./website && hugo server --disableFastRender",
    "catalogue:pdf": "node ./catalogue/pdf-generator/index.js"
  },
  "dependencies": {
    "@babel/cli": "^7.23.0",
    "@babel/core": "^7.23.0",
    "@babel/plugin-transform-runtime": "^7.22.15",
    "@babel/preset-env": "^7.22.20",
    "@rollup/plugin-commonjs": "^25.0.4",
    "@rollup/plugin-node-resolve": "^15.2.1",
    "autoprefixer": "^10.4.16",
    "byte-size": "^8.1.1",
    "date-fns": "^2.30.0",
    "dotenv": "^16.3.1",
    "eslint": "^8.50.0",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-standard": "^4.1.0",
    "front-matter": "^4.0.2",
    "isomorphic-git": "^1.24.5",
    "lunr": "^2.3.9",
    "lunr-languages": "^1.13.0",
    "marked": "^11.1.0",
    "npm-run-all": "^4.1.5",
    "pdfmake": "^0.2.7",
    "postcss-cli": "^10.1.0",
    "postcss-custom-media": "^10.0.1",
    "prettier": "^3.0.3",
    "rollup": "^3.29.4",
    "rollup-plugin-babel": "^4.4.0",
    "rollup-plugin-css-only": "^4.3.0",
    "rollup-plugin-livereload": "^2.0.5",
    "rollup-plugin-svelte": "^7.1.6",
    "rollup-plugin-terser": "^7.0.2",
    "shelljs": "^0.8.5",
    "sirv-cli": "^2.0.2",
    "ssh2": "^1.14.0",
    "standard": "^17.1.0",
    "svelte": "^4.2.19",
    "wavesurfer.js": "^7.3.3",
    "yaml": "^2.3.2"
  },
  "volta": {
    "node": "18.18.0"
  },
  "engines": {
    "node": "18.18.0"
  }
}
